//弾を生成するプレイヤーの宣言
using UnityEngine;

public class test : MonoBehaviour
{
	//弾の変数
	GameObject Bullet;
	//弾を管理する変数
	int count = 0;

	void Start()
	{
	}

	void Update()
	{
		if(Input.GetKey("i")){
			Bullet = Object.Instantiate(GameObject.Find("Bullet")) as GameObject;
			Bullet.transform.Translate(0,0,0);
			count++;
		}
	}
}
