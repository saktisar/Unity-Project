using UnityEngine;
using System.Collections;

public class Missaile : MonoBehaviour {
	// Use this for initialization
	void Start () {
		if(transform.position.x == -100){
			Destroy(this);
		}
	}
	
	// Update is called once per frame
	void Update () {
		//移動
		transform.Translate(0.0f,0.0f,1.0f);
	}
}
